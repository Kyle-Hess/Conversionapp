<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kyle.app.conversionapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_two"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.kyle.app.conversionapp.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.kyle.app.conversionapp.SettingsActivity"></activity>
    </application>
</manifest>